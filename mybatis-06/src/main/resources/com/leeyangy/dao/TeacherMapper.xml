        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.leeyangy.dao.TeacherMapper">
    <select id="getStudent" resultType="Student">
        select s.id,s.name,t.name from student s,teacher t where s.tid =tid
